<odoo>
    <data>
        <record id="view_account_config_settings_inherit" model="ir.ui.view">
            <field name="name">res.settings.inherit</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="child_compassion.compassion_settings"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='disaster_notify_ids']" position="after">
                    <div>
                        <label for="move_bills_date"/>
                        <field name="move_bills_date" class="oe_inline"/>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>





